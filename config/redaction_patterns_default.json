[
  {
    "id": "email",
    "regex": "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b",
    "color": "#000000",
    "enabled": true
  },
  {
    "id": "phone",
    "regex": "\\b\\d{3}[-.]?\\d{3}[-.]?\\d{4}\\b",
    "color": "#000000",
    "enabled": true
  },
  {
    "id": "date",
    "regex": "\\b\\d{1,2}[/-]\\d{1,2}[/-]\\d{2,4}\\b",
    "color": "#000000",
    "enabled": true
  },
  {
    "id": "percentage",
    "regex": "\\b\\d+\\.?\\d*%\\b",
    "color": "#000000",
    "enabled": true
  },
  {
    "id": "batch_id",
    "regex": "\\b[A-Z]{2,}-?\\d{4,}\\b",
    "color": "#000000",
    "enabled": true
  }
]