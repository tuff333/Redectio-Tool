{
  "company_id": "ppb",
  "display_name": "PPB Laboratories",

  "detection": {
    "text_contains": [
      "PPB",
      "PPB Laboratories",
      "PPB Labs",
      "PPB Inc",
      "PPB Testing",
      "PPB Certificate of Analysis"
    ],
    "priority": 8
  },

  "page_patterns": [
    {
      "page": "any",
      "match": [
        "PPB",
        "Certificate of Analysis",
        "COA",
        "Lot",
        "Sample ID",
        "Sample Name"
      ]
    }
  ],

  "rules": [
    {
      "field": "company_name",
      "patterns": [
        "PPB",
        "PPB Laboratories",
        "PPB Labs",
        "PPB Inc"
      ],
      "redact_type": "full"
    },
    {
      "field": "company_address",
      "patterns": [
        "\\d{2,5}\\s+[A-Za-z0-9\\s]+(Street|St|Road|Rd|Avenue|Ave|Boulevard|Blvd|Drive|Dr)"
      ],
      "redact_type": "full"
    },
    {
      "field": "company_phone",
      "patterns": [
        "\\(?\\d{3}\\)?[-\\s]?\\d{3}[-\\s]?\\d{4}"
      ],
      "redact_type": "full"
    },
    {
      "field": "certificate_number",
      "patterns": [
        "Certificate[:\\s]+[A-Za-z0-9\\-]+",
        "COA[:\\s]+[A-Za-z0-9\\-]+"
      ],
      "redact_type": "full"
    },
    {
      "field": "sample_id",
      "patterns": [
        "Sample ID[:\\s]+[A-Za-z0-9\\-]+"
      ],
      "redact_type": "full"
    },
    {
      "field": "lot_number",
      "patterns": [
        "Lot[:\\s]+[A-Za-z0-9\\-]+"
      ],
      "redact_type": "full"
    },
    {
      "field": "client_name",
      "patterns": [
        "Client[:\\s]+[A-Za-z0-9\\s\\-]+"
      ],
      "redact_type": "full"
    },
    {
      "field": "dates",
      "patterns": [
        "\\b\\d{4}-\\d{2}-\\d{2}\\b",
        "\\b\\d{2}/\\d{2}/\\d{4}\\b",
        "\\b\\d{1,2}-[A-Za-z]{3}-\\d{4}\\b"
      ],
      "redact_type": "partial"
    }
  ],

  "qr_code_regions": []
}
